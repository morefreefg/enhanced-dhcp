{
	"luci-app-enhanced-dhcp-v2": {
		"description": "Grant access to Enhanced DHCP Manager v2.0",
		"read": {
			"uci": [ "enhanced_dhcp", "dhcp", "network" ],
			"file": {
				"/var/dhcp.leases": [ "read" ],
				"/proc/net/arp": [ "read" ],
				"/etc/config/dhcp": [ "read" ],
				"/etc/config/enhanced_dhcp": [ "read" ]
			}
		},
		"write": {
			"uci": [ "enhanced_dhcp", "dhcp" ],
			"file": {
				"/etc/config/dhcp": [ "write" ],
				"/etc/config/enhanced_dhcp": [ "write" ]
			},
			"exec": {
				"/etc/init.d/dnsmasq": [ "restart", "reload" ],
				"/etc/init.d/enhanced_dhcp": [ "start", "stop", "restart", "reload" ]
			}
		}
	}
}